{
  "manifest_version": 3,
  "name": "PoE Currency Tracker",
  "version": "0.1",
  "permissions": ["storage", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.pathofexile.com/trade2/history*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
